{
  "domains": [
    {
      "name": "demo1",
      "enable": true,
      "domain": "",
      "sub": [
        "https://raw.githubusercontent.com/dongchengjie/airport/refs/heads/main/subs/merged/tested_within.yaml"
      ],
      "push_to": [
        "sub1"
      ]
    },
    {
      "name": "demo2",
      "enable": true,
      "domain": "",
      "sub": [
        "https://raw.githubusercontent.com/Leon406/SubCrawler/refs/heads/main/sub/share/vless"
      ],
      "push_to": [
        "sub1"
      ]
    },
    {
      "name": "demo3",
      "enable": true,
      "domain": "",
      "sub": [
        "https://raw.githubusercontent.com/Leon406/SubCrawler/refs/heads/main/sub/share/hysteria2"
      ],
      "push_to": [
        "sub1"
      ]
    }
  ],
  "crawl": {
    "enable": true,
    "exclude": "",
    "threshold": 5,
    "singlelink": true,
    "persist": {
      "subs": "crawledsubs",
      "proxies": "crawledproxies"
    },
    "config": {
      "rename": "",
      "include": "",
      "exclude": "",
      "xxxxxxx": ""
    },
    "telegram": {
      "enable": true,
      "pages": 6,
      "exclude": "",
      "users": {
        "channel": {
          "include": "机场,clash,订阅,节点,proxy,科学上网,v2ray,vless,trojan,socks5,ssr,白嫖",
          "exclude": "试用,免费,测试,失效,过期,广告,推广,加群",
          "config": {
            "rename": "TG机场源"
          },
          "push_to": [
            "sub1"
          ]
        }
      }
    },
    "github": {
      "enable": true,
      "pages": 2,
      "exclude": "",
      "push_to": [
        "sub1"
      ],
      "spams": []
    }
  },
  "groups": {
    "sub1": {
      "emoji": true,
      "list": true,
      "format": {
        "escape": true
      },
      "targets": {
        "clash": "sub1-clash",
        "singbox": "sub1-singbox",
        "v2ray": "sub1-v2ray"
      },
      "regularize": {
        "enable": false,
        "locate": true,
        "residential": true,
        "bits": 2
      }
    }
  },
  "storage": {
    "engine": "gist",
    "items": {
      "sub1-clash": {
        "username": "{{MY_GIST_USERNAME}}",
        "gistid": "{{MY_GIST_ID}}",
        "filename": "sub1-clash.yaml"
      },
      "sub1-singbox": {
        "username": "{{MY_GIST_USERNAME}}",
        "gistid": "{{MY_GIST_ID}}",
        "filename": "sub1-singbox.json"
      },
      "sub1-v2ray": {
        "username": "{{MY_GIST_USERNAME}}",
        "gistid": "{{MY_GIST_ID}}",
        "filename": "sub1-v2ray.txt"
      },
      "crawledsubs": {
        "username": "{{MY_GIST_USERNAME}}",
        "gistid": "{{MY_GIST_ID}}",
        "filename": "crawledsubs.yaml"
      },
      "crawledproxies": {
        "username": "{{MY_GIST_USERNAME}}",
        "gistid": "{{MY_GIST_ID}}",
        "filename": "crawledproxies.yaml"
      }
    }
  }
}
